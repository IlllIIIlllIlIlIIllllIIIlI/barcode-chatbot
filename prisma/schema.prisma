datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Chatter {
  username           String   @id @unique
  failedPyramids     Int
  successfulPyramids Int
  createdOn          DateTime @default(now())
  modifiedOn         DateTime @updatedAt
}
